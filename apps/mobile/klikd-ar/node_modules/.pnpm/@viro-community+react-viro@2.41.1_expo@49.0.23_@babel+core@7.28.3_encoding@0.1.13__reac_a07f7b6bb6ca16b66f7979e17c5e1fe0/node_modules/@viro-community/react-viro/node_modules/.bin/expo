#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mohammeds/Desktop/Klikd/apps/mobile/klikd-ar/node_modules/.pnpm/expo@49.0.23_@babel+core@7.28.3_encoding@0.1.13/node_modules/expo/bin/node_modules:/Users/mohammeds/Desktop/Klikd/apps/mobile/klikd-ar/node_modules/.pnpm/expo@49.0.23_@babel+core@7.28.3_encoding@0.1.13/node_modules/expo/node_modules:/Users/mohammeds/Desktop/Klikd/apps/mobile/klikd-ar/node_modules/.pnpm/expo@49.0.23_@babel+core@7.28.3_encoding@0.1.13/node_modules:/Users/mohammeds/Desktop/Klikd/apps/mobile/klikd-ar/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mohammeds/Desktop/Klikd/apps/mobile/klikd-ar/node_modules/.pnpm/expo@49.0.23_@babel+core@7.28.3_encoding@0.1.13/node_modules/expo/bin/node_modules:/Users/mohammeds/Desktop/Klikd/apps/mobile/klikd-ar/node_modules/.pnpm/expo@49.0.23_@babel+core@7.28.3_encoding@0.1.13/node_modules/expo/node_modules:/Users/mohammeds/Desktop/Klikd/apps/mobile/klikd-ar/node_modules/.pnpm/expo@49.0.23_@babel+core@7.28.3_encoding@0.1.13/node_modules:/Users/mohammeds/Desktop/Klikd/apps/mobile/klikd-ar/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../expo@49.0.23_@babel+core@7.28.3_encoding@0.1.13/node_modules/expo/bin/cli" "$@"
else
  exec node  "$basedir/../../../../../../expo@49.0.23_@babel+core@7.28.3_encoding@0.1.13/node_modules/expo/bin/cli" "$@"
fi
