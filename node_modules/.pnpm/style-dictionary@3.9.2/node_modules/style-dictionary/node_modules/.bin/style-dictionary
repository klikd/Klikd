#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mohammeds/Desktop/Klikd/node_modules/.pnpm/style-dictionary@3.9.2/node_modules/style-dictionary/bin/node_modules:/Users/mohammeds/Desktop/Klikd/node_modules/.pnpm/style-dictionary@3.9.2/node_modules/style-dictionary/node_modules:/Users/mohammeds/Desktop/Klikd/node_modules/.pnpm/style-dictionary@3.9.2/node_modules:/Users/mohammeds/Desktop/Klikd/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mohammeds/Desktop/Klikd/node_modules/.pnpm/style-dictionary@3.9.2/node_modules/style-dictionary/bin/node_modules:/Users/mohammeds/Desktop/Klikd/node_modules/.pnpm/style-dictionary@3.9.2/node_modules/style-dictionary/node_modules:/Users/mohammeds/Desktop/Klikd/node_modules/.pnpm/style-dictionary@3.9.2/node_modules:/Users/mohammeds/Desktop/Klikd/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/style-dictionary" "$@"
else
  exec node  "$basedir/../../bin/style-dictionary" "$@"
fi
